{
  "manifest_version": 3,
  "name": "JavDB Viewed Marker",
  "version": "1.7.0",
  "description": "Marks viewed videos on JavDB and provides advanced filtering and data management.",
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "scripting"
  ],
  "host_permissions": [
    "*://*.javdb.com/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-CBEmlLJ-.js"
      ],
      "matches": [
        "*://*.javdb.com/*"
      ]
    }
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/assets/favicon-16x16.png",
      "48": "src/assets/favicon-16x16.png",
      "128": "src/assets/favicon-16x16.png"
    }
  },
  "icons": {
    "16": "src/assets/favicon-16x16.png",
    "48": "src/assets/favicon-16x16.png",
    "128": "src/assets/favicon-16x16.png"
  },
  "options_ui": {
    "page": "src/dashboard/dashboard.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://*.javdb.com/*"
      ],
      "resources": [
        "src/assets/jav.png",
        "assets/storage-BU9Pj700.js",
        "assets/content.ts-B1hHhRi6.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
